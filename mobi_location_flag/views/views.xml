<odoo>
    <data>
        <record id="view_inherit_stock_view_location_form_form_form" model="ir.ui.view">
            <field name="name">view.inherit.stock.view.location.form.form</field>
            <field name="model">stock.location</field>
            <field name="inherit_id" ref="stock.view_location_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='usage']" position="after">
                    <field name="is_internal_location"/>
                </xpath>
            </field>
        </record>

    </data>
</odoo>